{
  "name": "css-frameworks-ca",
  "version": "1.0.13",
  "description": "Test",
  "main": "index.js",
  "scripts": {
    "build": "sass src/scss:dist/css",
    "start": "sass --watch src/scss:dist/css & live-server",
    "prepare": "husky install",
    "test-jest": " npm run unit-test",
    "unit-test": "jest",
    "e2e-cli": "npm run cypress-test",
    "e2e-browser": "cypress open",
    "cypress-test": "cypress run",
    "format-js": "prettier -w src/js/**/*.js",
    "format-scss": "prettier -w src/scss/*.scss",
    "format-html": "prettier -w *.html",
    "eslint": "eslint src/**/*.js",
    "eslint-fix": "eslint src/**/*.js --cache --fix"
  },
  "keywords": [],
  "author": "Noroff",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.19.3",
    "@babel/preset-env": "^7.19.4",
    "cypress": "^13.4.0",
    "eslint": "^8.52.0",
    "eslint-plugin-cypress": "^2.15.1",
    "eslint-plugin-jest": "^27.6.0",
    "husky": "^8.0.3",
    "lint-staged": "^15.0.2",
    "live-server": "^1.2.2",
    "prettier": "^3.0.3",
    "sass": "^1.54.8"
  },
  "dependencies": {
    "bootstrap-dark-5": "^1.1.3",
    "jest": "^29.7.0"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.html": [
      "prettier --write"
    ],
    "*.scss": [
      "prettier --write"
    ]
  }
}
